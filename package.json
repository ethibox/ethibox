{
  "name": "Ethibox",
  "homepage": "https://ethibox.fr",
  "description": "Open-source web apps hoster",
  "version": "1.1.0",
  "author": "@ethibox",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/runtime": "^7.12.5",
    "@prisma/client": "~2.11.0",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "graphql": "^15.4.0",
    "if-env": "^1.0.4",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "matomo-tracker": "^2.2.4",
    "mjml": "^4.7.1",
    "nodemailer": "^6.4.17",
    "prom-client": "^12.0.0",
    "stripe": "^8.129.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@prisma/cli": "~2.11.0",
    "@tailwindcss/ui": "^0.7.2",
    "babel-plugin-import-graphql": "^2.7.0",
    "blueimp-md5": "^2.18.0",
    "cypress": "^5.6.0",
    "cypress-dotenv": "^1.2.2",
    "cypress-file-upload": "^4.1.1",
    "cypress-localstorage-commands": "^1.3.1",
    "cypress-wait-until": "^1.7.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "fetch-mock-jest": "^1.5.1",
    "gatsby": "^2.29.1",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.9.0",
    "gatsby-plugin-matomo": "^0.8.3",
    "gatsby-plugin-offline": "^3.7.0",
    "gatsby-plugin-posthog": "^1.0.1",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-plugin-sharp": "^2.11.1",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "graphql-hooks": "^4.5.0",
    "immer": "^8.0.1",
    "jest": "^26.6.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-medium-image-zoom": "^3.1.2",
    "react-router": "^5.2.0",
    "react-sweet-state": "^2.5.1",
    "recoil": "^0.1.2",
    "start-server-and-test": "^1.11.6",
    "tailwindcss": "^1.9.6",
    "use-immer": "^0.4.2"
  },
  "keywords": [
    "gatsby",
    "ethibox"
  ],
  "license": "GPL-3.0",
  "scripts": {
    "start": "if-env NODE_ENV=production && gatsby serve || npm run start:dev",
    "start:dev": "npm-run-all --parallel watch:server watch:client",
    "start:prod": "NODE_ENV=production node public/index.js",
    "watch:client": "gatsby develop -p 8080",
    "watch:server": "nodemon -e js,graphql -w 'src/server/**/*' -i src/server/test --exec babel-node -- src/server/index.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "NODE_ENV=production babel --minified --no-comments --compact true -d public/ src/server",
    "build:client": "gatsby build",
    "migrate": "prisma generate && prisma migrate save -c --experimental --name migration && prisma migrate up -c --auto-approve --experimental",
    "generate": "prisma generate",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" && eslint --fix src/**/*.js",
    "clean": "gatsby clean",
    "test": "npm run test:init && npm run test:unit && npm run test:e2e",
    "test:init": "npm run migrate",
    "test:unit": "jest --testTimeout=30000 --forceExit -i src/server/**/*.spec.js",
    "test:ci": "CI=true npm run test:init && start-test 8080",
    "test:e2e": "cypress run",
    "studio": "prisma studio --experimental"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethibox/ethibox"
  },
  "bugs": {
    "url": "https://github.com/ethibox/ethibox/issues"
  }
}
