{
  "name": "Ethibox",
  "homepage": "https://github.com/ethibox/ethibox/",
  "description": "Open-source web apps hoster",
  "version": "1.2.0",
  "author": "@ethibox",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.15.3",
    "@prisma/client": "~2.29.1",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.17.1",
    "apollo-server-express": "^2.25.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-ipfilter": "^1.2.0",
    "generate-password": "^1.6.1",
    "graphql": "^15.5.1",
    "if-env": "^1.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "prom-client": "^13.2.0",
    "stripe": "^8.170.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@graphql-eslint/eslint-plugin": "^2.0.2",
    "@reach/router": "^1.3.4",
    "@tailwindcss/forms": "^0.3.3",
    "babel-plugin-import-graphql": "^2.8.1",
    "base-64": "^1.0.0",
    "cypress": "^8.3.0",
    "cypress-dotenv": "^1.2.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-wait-until": "^1.7.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "fetch-mock-jest": "^1.5.1",
    "gatsby": "^3.12.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-offline": "^4.12.0",
    "gatsby-plugin-posthog": "^1.0.1",
    "gatsby-plugin-purgecss": "^6.0.2",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sass": "^4.12.0",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-transformer-sharp": "^3.12.0",
    "immer": "^9.0.6",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prisma": "^2.29.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "recoil": "^0.4.0",
    "start-server-and-test": "^1.13.1",
    "tailwindcss": "^2.2.7",
    "use-immer": "^0.6.0"
  },
  "keywords": [
    "gatsby",
    "ethibox"
  ],
  "license": "GPL-3.0",
  "scripts": {
    "start": "if-env NODE_ENV=production && gatsby serve || npm run start:dev",
    "start:dev": "npm-run-all --parallel watch:server watch:client",
    "start:prod": "NODE_ENV=production node public/index.js",
    "watch:client": "gatsby develop -p 8080",
    "watch:server": "nodemon -e js,graphql -w 'src/server/**/*' -i src/server/test --exec babel-node -- src/server/index.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "NODE_ENV=production babel --minified --no-comments --compact true -d public/ src/server",
    "build:client": "gatsby build",
    "migrate": "prisma generate && prisma db push --accept-data-loss",
    "generate": "prisma generate",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" && eslint --fix src/**/*.js",
    "clean": "gatsby clean",
    "test": "npm run test:init && npm run test:unit && npm run test:e2e",
    "test:init": "npm run migrate",
    "test:unit": "jest --testTimeout=30000 --forceExit -i src/server/**/*.spec.js",
    "test:ci": "CI=true npm run test:init && start-test 8080",
    "test:e2e": "cypress run",
    "studio": "prisma studio --experimental"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethibox/ethibox"
  },
  "bugs": {
    "url": "https://github.com/ethibox/ethibox/issues"
  }
}
